{
  "TestCaseName": "Action: Durable Function / Hierarchical Seed",
  "Arrange": [
    {
      "Strategy": "JsonFileSeed",
      "Parameters": {
        "table": "Products",
        "dataFile": "data/products.json",
        "allowIdentityInsert": true
      }
    },
    {
      "Strategy": "HierarchicalSeed",
      "Parameters": {
        "dataFile": "data/new-customer-order.json",
        "Outputs": [
          {
            "VariableName": "newCustomerId",
            "Source": {
              "FromTable": "Customers",
              "SelectColumn": "CustomerId",
              "OrderByColumn": "CustomerId",
              "OrderDirection": "DESC"
            }
          }
        ]
      }
    }
  ],
  "Actions": [
    {
      "Strategy": "DurableFunction",
      "Parameters": {
        "OrchestratorName": "RunOrchestrator",
        "Payload": {
          "CustomerId": "{newCustomerId}",
          "NewEmail": "alice.smith@example.com"
        },
        "PollingTimeoutSeconds": 120,
        "ExpectedStatus": "Completed"
      }
    }
  ],
  "Assert": [
    {
      "TableName": "Customers",
      "ExpectedDataFile": "expected\\customer-email-changed.json",
      "ComparisonStrategy": "IgnoreColumns",
      "ComparisonParameters": [ "CustomerId" ]
    }
  ]
}